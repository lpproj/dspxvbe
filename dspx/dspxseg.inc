;
; dspxseg.inc
;

	.286
	.XLIST

TSR_IDENTSEG	SEGMENT PARA PUBLIC USE16 'TSR'
TSR_IDENTSEG	ENDS
TSR_TEXT	SEGMENT	PARA PUBLIC USE16 'TSR'		; Resident code
TSR_TEXT	ENDS
G_DATA		SEGMENT	DWORD PUBLIC USE16 'TSR'	; Data (Non-Local)
G_DATA		ENDS
LOCALTOPSEG	SEGMENT	DWORD PUBLIC USE16 'TSR'
LOCALTOPSEG	ENDS
L_DATA		SEGMENT DWORD PUBLIC USE16 'TSR'	; Data (Local)
L_DATA		ENDS
LOCALENDSEG	SEGMENT	DWORD PUBLIC USE16 'TSR'
LOCALENDSEG	ENDS
F_DATA		SEGMENT	DWORD PUBLIC USE16 'TSR'	; Font(?)
F_DATA		ENDS
TSRENDSEG	SEGMENT	PARA PUBLIC USE16 'TSR'
TSRENDSEG	ENDS
_TEXT		SEGMENT	WORD PUBLIC USE16 'CODE'	; Non-Resident Code
_TEXT		ENDS
_DATA		SEGMENT	WORD PUBLIC USE16 'DATA'	; Non-Resident Data
_DATA		ENDS
_BSS		SEGMENT	WORD PUBLIC USE16 'BSS'		; Non-Resident Bss
_BSS		ENDS

	IFDEF	??version
	NOWARN	RES
	ENDIF

STACK		SEGMENT	PARA STACK USE16 'STACK'
STACK		ENDS

	IFDEF	??version
	WARN	RES
	ENDIF



AGROUP		GROUP	TSR_IDENTSEG, TSR_TEXT, G_DATA, LOCALTOPSEG, L_DATA,\
			LOCALENDSEG,F_DATA, TSRENDSEG, _TEXT, _DATA, _BSS,\
			STACK

ofs		EQU	offset AGROUP:

		ASSUME	cs:AGROUP,ds:AGROUP,es:AGROUP,ss:AGROUP

LOCALTOPSEG	SEGMENT
	ORG	0
TopOfLData	LABEL	BYTE
LOCALTOPSEG	ENDS


LOCALENDSEG	SEGMENT
	ORG	0
BottomOfLData	LABEL	BYTE
TopOfFont	LABEL	BYTE
LOCALENDSEG	ENDS


TSRENDSEG	SEGMENT
	ORG	0
BottomOfTsr	LABEL	BYTE
TSRENDSEG	ENDS

	.LIST

	IFDEF	??version
	LOCALS
	ENDIF

